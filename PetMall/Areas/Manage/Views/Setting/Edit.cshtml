                    <input asp-for="@Model.Email" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.Email"></span>
                    <hr />
                    <label asp-for="@Model.OpenTime" class="form-label">Open Time</label>
                    <input asp-for="@Model.OpenTime" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.OpenTime"></span>
                    <hr />
                    <label asp-for="@Model.OurCompany" class="form-label">Our Company</label>
                    <input asp-for="@Model.OurCompany" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.OurCompany"></span>
                    <hr />
                    <label asp-for="@Model.OurTeam" class="form-label">Our Team</label>
                    <input asp-for="@Model.OurTeam" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.OurTeam"></span>
                    <hr />
                    <label asp-for="@Model.CompanyQuote" class="form-label">Company Quote</label>
                    <input asp-for="@Model.CompanyQuote" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.CompanyQuote"></span>
                    <hr />
                    <label asp-for="@Model.Conditions" class="form-label">Conditions</label>
                    <input asp-for="@Model.Conditions" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.Conditions"></span>
                    <hr />
                    <label asp-for="@Model.Cookies" class="form-label">Cookies</label>
                    <input asp-for="@Model.Cookies" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.Cookies"></span>
                    <hr />
                    <label asp-for="@Model.PrivacyPolicy" class="form-label">Privacy Policy</label>
                    <input asp-for="@Model.PrivacyPolicy" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.PrivacyPolicy"></span>
                </div>
                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
    </div>
</div>

@section Scripts{
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js" integrity="sha512-37T7leoNS06R80c8Ulq7cdCDU5MNQBwlYoy1TX/WUsLFC2eYNqtKlV0QjH7r8JpG/S0GUMZwebnVFLPd6SU5yg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.12/jquery.validate.unobtrusive.min.js" integrity="sha512-o6XqxgrUsKmchwy9G5VRNWSSxTS4Urr4loO6/0hYdpWmFUfHqGzawGxeQGMDqYzxjY9sbktPbNlkIQJWagVZQg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
}