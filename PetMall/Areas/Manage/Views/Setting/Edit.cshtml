                    <hr />
                    <label asp-for="@Model.TweeterUrl" class="form-label">Tweeter URL</label>
                    <input asp-for="@Model.TweeterUrl" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.TweeterUrl"></span>
                    <hr />
                    <label asp-for="@Model.Address" class="form-label">Address</label>
                    <input asp-for="@Model.Address" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.Address"></span>
                    <hr />
