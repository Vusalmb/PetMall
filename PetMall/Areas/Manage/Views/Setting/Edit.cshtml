@model Setting
@{
    ViewData["Title"] = "Edit";
}

<div class="container">
    <div class="row">
        <div class="mt-5">
            <h2 class="mb-4">Edit Setting Section</h2>
            <div asp-validation-summary="ModelOnly" class="alert alert-danger"></div>
            <form method="post" class="mt-3" enctype="multipart/form-data">
                <div class="mb-3">
                    <label asp-for="@Model.LogoFile" class="form-label">Logo</label>
                    <input asp-for="@Model.LogoFile" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.LogoFile"></span>
                    <hr />
                    <label asp-for="@Model.LogoImageFile" class="form-label">Logo Image</label>
                    <input asp-for="@Model.LogoImageFile" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.LogoImageFile"></span>
                    <hr />
                    <label asp-for="@Model.LogoDesc" class="form-label">Logo Desc</label>
                    <input asp-for="@Model.LogoDesc" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.LogoDesc"></span>
                    <hr />
                    <label asp-for="@Model.SearchIcon" class="form-label">Search Icon</label>
                    <input asp-for="@Model.SearchIcon" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.SearchIcon"></span>
                    <hr />
                    <label asp-for="@Model.BasketIcon" class="form-label">Basket Icon</label>
                    <input asp-for="@Model.BasketIcon" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.BasketIcon"></span>
                    <hr />
                    <label asp-for="@Model.SettingIcon" class="form-label">Setting Icon</label>
                    <input asp-for="@Model.SettingIcon" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.SettingIcon"></span>
                    <hr />
                    <label asp-for="@Model.ScrollTopIcon" class="form-label">ScrollTop Icon</label>
                    <input asp-for="@Model.ScrollTopIcon" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.ScrollTopIcon"></span>
                    <hr />
                    <label asp-for="@Model.FacebookIcon" class="form-label">Facebook Icon</label>
                    <input asp-for="@Model.FacebookIcon" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.FacebookIcon"></span>
                    <hr />
                    <label asp-for="@Model.FacebookUrl" class="form-label">Facebook URL</label>
                    <input asp-for="@Model.FacebookUrl" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.FacebookUrl"></span>
                    <hr />
                    <label asp-for="@Model.PinterestIcon" class="form-label">Pinterest Icon</label>
                    <input asp-for="@Model.PinterestIcon" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.PinterestIcon"></span>
                    <hr />
                    <label asp-for="@Model.PinterestUrl" class="form-label">Pinterest URL</label>
                    <input asp-for="@Model.PinterestUrl" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.PinterestUrl"></span>
                    <hr />
                    <label asp-for="@Model.InstagramIcon" class="form-label">Instagram Icon</label>
                    <input asp-for="@Model.InstagramIcon" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.InstagramIcon"></span>
                    <hr />
                    <label asp-for="@Model.InstagramUrl" class="form-label">Instagram URL</label>
                    <input asp-for="@Model.InstagramUrl" class="form-control">
                    <span style="display: inline-block; margin-top: 8px;" class="text text-danger" asp-validation-for="@Model.InstagramUrl"></span>
                    <hr />
                    <label asp-for="@Model.TweeterIcon" class="form-label">Tweeter Icon</label>
